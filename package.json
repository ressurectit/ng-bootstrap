{
    "name": "@anglr/bootstrap",
    "version": "3.0.0-beta.5",
    "description": "Angular module wrapping bootstrap components",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "scripts":
    {
        "build": "npm run ngc",
        "clean": "rimraf dist",
        "clean:deps": "rimraf node_modules",
        "clean:all": "npm run clean && npm run clean:deps",
        "ngc": "ngc",
        "prepublish": "npm run bundle:package",
        "postpublish": "npm run clean",
        "republish": "npm publish --force",
        "bundle": "webpack",
        "bundle:min": "webpack --env.minify",
        "bundle:package": "npm run clean && npm run build && npm run bundle && npm run bundle:min",
        "deploy:stable": "npp -b",
        "deploy:next": "npp -p -b -s \"beta\" -t \"next\""
    },
    "keywords": 
    [
        "angular",
        "ng",
        "bootstrap",
        "components"
    ],
    "author": "kukjevov",
    "license": "MIT",
    "dependencies":
    {
        "@angular/core": ">=4.0.0-rc.5",
        "@angular/common": ">=4.0.0-rc.5",
        "@angular/forms": ">=4.0.0-rc.5",
        "core-js": "~2.4.1",
        "zone.js": "^0.8.4",
        "rxjs": "^5.2.0",
        "@anglr/common": ">=3.0.0-beta.6"
    },
    "peerDependencies":
    {
        "bootstrap": "~3.3.7",
        "jquery": "~2.2.1",
        "moment": "~2.16.0",
        "bootstrap-select": "1.12.1",
        "eonasdan-bootstrap-datetimepicker": "^4.17.47",
        "typeahead.js": "~0.11.1",
        "handlebars": "~4.0.6"
    },
    "devDependencies":
    {
        "@types/jquery": "~2.0.40",
        "@types/bootstrap": "~3.3.32",
        "@types/eonasdan-bootstrap-datetimepicker": "~4.17.22",
        "@types/typeahead": "~0.11.27",
        "@types/handlebars": "~4.0.31",
        "@anglr/types": "~1.1.2",
        "@angular/compiler-cli": ">=4.0.0-rc.5",
        "@angular/compiler": ">=4.0.0-rc.5",
        "rimraf": "~2.6.1",
        "typescript": "2.2.1",
        "webpack": "2.2.1",
        "npm-publish-package": "~1.2.0"
    }
}