{
    "name": "@ng2/bootstrap-components",
    "version": "1.1.1",
    "description": "Angular 2 module wrapping bootstrap components",
    "main": "index.js",
    "typings": "index.d.ts",
    "scripts":
    {
        "prebuild": "typings install && gulp copy-tstypings",
        "build": "npm run tsc",
        "clean": "rimraf ./*.d.ts ./index.js ./index.dev.js ./index.min.js ./*.map ./*.log ./components ./directives",
        "clean:deps": "rimraf ./node_modules ./typings",
        "clean:all": "npm run clean && npm run clean:deps",
        "tsc": "tsc",
        "tsc:w": "tsc -w --sourceMap",
        "tsc:debug": "tsc --sourceMap",
        "prepublish": "npm run bundle:package",
        "postpublish": "npm run clean",
        "republish": "npm publish --force",
        "bundle": "tsc --outFile ./index.dev.js",
        "bundle:update": "nab ./index.dev.js && uglifyjs -o ./index.min.js -- ./index.dev.js",
        "bundle:package": "npm run clean && npm run build && npm run bundle && npm run bundle:update",
        "deploy-stable": "npp -b",
        "deploy-next": "npp -p -b -s \"beta\" -t \"next\""
    },
    "keywords": [
        "angular2",
        "ng2",
        "bootstrap",
        "components"
    ],
    "author": "kukjevov",
    "license": "MIT",
    "dependencies":
    {
        "@angular/core":  "2.0.0-rc.2",
        "@angular/compiler":  "2.0.0-rc.2",
        "@angular/common":  "2.0.0-rc.2",
        "es6-promise": "^3.1.2",
        "es6-shim": "^0.35.0",
        "reflect-metadata": "0.1.3",
        "zone.js": "0.6.12",
        "rxjs": "5.0.0-beta.6",
        "bootstrap": "~3.3.6",
        "jquery": "~2.2.1",
        "moment": "~2.12.0",
        "bootstrap-select": "~1.10.0",
        "eonasdan-bootstrap-datetimepicker": "^4.15.35",
        "@ng2/common": "1.1.0"
    },
    "devDependencies":
    {
        "rimraf": "~2.5.2",
        "typescript": "1.8.10",
        "typings":"~0.7.12",
        "uglify-js": "~2.6.2",
        "gulp": "~3.9.1",
        "ng2-tstypings": "1.0.3",
        "npm-publish-package": "1.2.0",
        "npm-angular2-bundle": "1.0.1"
    }
}