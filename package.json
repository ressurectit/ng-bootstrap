{
    "name": "@anglr/bootstrap",
    "version": "6.1.1",
    "description": "Angular module wrapping bootstrap components",
    "module": "./dist/es5/bootstrap.js",
    "esm2015": "./dist/es2015/bootstrap.js",
    "typings": "./bootstrap.d.ts",
    "sideEffects": false,
    "scripts": {
        "build": "npm run build:core && npm run build:datetimepicker && npm run build:select && npm run build:switch && npm run build:typeahead && npm run build:bootstrap",
        "build:bootstrap": "ngc -p tsconfig.es5.json && ngc -p tsconfig.es2015.json",
        "build:core": "ngc -p core/tsconfig.es5.json && ngc -p core/tsconfig.es2015.json",
        "build:datetimepicker": "ngc -p datetimepicker/tsconfig.es5.json && ngc -p datetimepicker/tsconfig.es2015.json",
        "build:select": "ngc -p select/tsconfig.es5.json && ngc -p select/tsconfig.es2015.json",
        "build:switch": "ngc -p switch/tsconfig.es5.json && ngc -p switch/tsconfig.es2015.json",
        "build:typeahead": "ngc -p typeahead/tsconfig.es5.json && ngc -p typeahead/tsconfig.es2015.json",
        "clean": "rimraf dist tests docs src/**/*.d.ts core/**/*.d.ts datetimepicker/**/*.d.ts select/**/*.d.ts switch/**/*.d.ts typeahead/**/*.d.ts */*.d.ts */*.metadata.json ./*.metadata.json ./*.d.ts */dist",
        "clean:duplicates": "rimraf */dist/*/src dist/*/core dist/*/datetimepicker dist/*/select dist/*/switch dist/*/typeahead",
        "clean:deps": "rimraf package-lock.json node_modules",
        "clean:all": "npm run clean && npm run clean:deps",
        "ngc": "ngc",
        "version:placeholder": "replace-in-file /\\\"version\\\":\\s\\\".*?\\\"/g \"\\\"version\\\": \\\"0.0.0-placeholder\\\"\" package.json --isRegex",
        "version:git": "ngv -e \"replace-in-file 0.0.0-placeholder $GIT_VERSION package.json\"",
        "prepublishOnly": "npm run bundle && npm run version:placeholder && npm run version:git",
        "postpublish": "npm run version:placeholder && npm run clean",
        "bundle": "npm run clean && npm run build && npm run clean:duplicates",
        "deploy": "npm publish",
        "deploy:next": "npm publish --tag next",
        "pretest": "webpack",
        "test": "karma start karma.conf.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ressurectit/ng-bootstrap.git"
    },
    "keywords": [
        "angular",
        "ng",
        "bootstrap",
        "components"
    ],
    "author": "kukjevov",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ressurectit/ng-bootstrap/issues"
    },
    "homepage": "https://github.com/ressurectit/ng-bootstrap",
    "peerDependencies": {
        "@angular/core": ">=8.0.0",
        "@angular/common": ">=8.0.0",
        "@angular/forms": ">=8.0.0",
        "rxjs": ">=6.5.0",
        "@jscrpt/common": ">=1.0.2",
        "@anglr/common": ">=6.2.0",
        "bootstrap": "^3.3.7",
        "jquery": "^3.3.1",
        "moment": "^2.22.1",
        "bootstrap-select": "^1.12.4",
        "eonasdan-bootstrap-datetimepicker": "^4.17.47",
        "typeahead.js": "^0.11.1",
        "handlebars": "^4.0.11",
        "bootstrap-switch": "^3.3.4"
    },
    "devDependencies": {
        "@angular/core": "^8.0.0",
        "@angular/common": "^8.0.0",
        "@angular/forms": "^8.0.0",
        "rxjs": "^6.5.0",
        "@jscrpt/common": "^1.0.2",
        "@anglr/common": "^6.2.0",
        "@anglr/types": "1.1.17",
        "@types/jasmine": "~2.8.7",
        "@types/jquery": "~3.3.1",
        "@types/bootstrap": "~3.3.36",
        "@types/eonasdan-bootstrap-datetimepicker": "~4.17.26",
        "@types/typeahead": "~0.11.32",
        "@types/handlebars": "~4.1.0",
        "@types/bootstrap-switch": "0.0.31",
        "@angular/router": "^8.0.0",
        "@angular/compiler-cli": "^8.0.0",
        "@angular/compiler": "^8.0.0",
        "@angular/language-service": "^8.0.0",
        "@angular/platform-browser-dynamic": "^8.0.0",
        "@angular/platform-browser": "^8.0.0",
        "tsickle": "0.35.0",
        "karma-cli": "1.0.1",
        "karma": "3.1.1",
        "karma-jasmine": "1.1.2",
        "karma-phantomjs-launcher": "1.0.4",
        "karma-chrome-launcher": "2.2.0",
        "karma-webpack": "3.0.5",
        "karma-jasmine-html-reporter": "1.4.0",
        "jasmine": "3.3.0",
        "awesome-typescript-loader": "5.2.1",
        "angular2-template-loader": "0.6.2",
        "raw-loader": "1.0.0",
        "sass-loader": "7.1.0",
        "css-loader": "1.0.1",
        "file-loader": "3.0.1",
        "style-loader": "0.23.1",
        "@compodoc/compodoc": "1.1.9",
        "rimraf": "2.6.3",
        "copyfiles": "2.1.0",
        "replace-in-file": "4.1.0",
        "typescript": "3.4.5",
        "webpack": "4.32.2",
        "npm-git-version": "1.1.1"
    }
}
