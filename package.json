{
    "name": "@ng2/bootstrap-components",
    "version": "2.0.7",
    "description": "Angular 2 module wrapping bootstrap components",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "scripts":
    {
        "build": "npm run tsc",
        "clean": "rimraf dist",
        "clean:deps": "rimraf node_modules",
        "clean:all": "npm run clean && npm run clean:deps",
        "tsc": "tsc",
        "tsc:w": "tsc -w --sourceMap",
        "tsc:debug": "tsc --sourceMap",
        "prepublish": "npm run bundle:package",
        "postpublish": "npm run clean",
        "republish": "npm publish --force",
        "bundle": "webpack",
        "bundle:min": "webpack --env.minify",
        "bundle:package": "npm run clean && npm run build && npm run bundle && npm run bundle:min",
        "deploy-stable": "npp -b",
        "deploy-next": "npp -p -b -s \"beta\" -t \"next\""
    },
    "keywords": [
        "angular2",
        "ng2",
        "bootstrap",
        "components"
    ],
    "author": "kukjevov",
    "license": "MIT",
    "dependencies":
    {
        "@angular/core": "~2.0.2",
        "@angular/common": "~2.0.2",
        "@angular/compiler": "~2.0.2",
        "@angular/forms": "~2.0.2",
        "core-js": "~2.4.1",
        "reflect-metadata": "~0.1.3",
        "zone.js": "~0.6.23",
        "rxjs": "5.0.0-beta.12",
        "@ng2/common": "~2.0.1"
    },
    "peerDependencies":
    {
        "bootstrap": "~3.3.7",
        "jquery": "~2.2.1",
        "moment": "~2.16.0",
        "bootstrap-select": "1.11.3",
        "eonasdan-bootstrap-datetimepicker": "~4.17.43",
        "typeahead.js": "~0.11.1",
        "handlebars": "~4.0.6"
    },
    "devDependencies":
    {
        "@types/jquery": "~2.0.32",
        "@types/bootstrap": "~3.3.32",
        "@types/eonasdan-bootstrap-datetimepicker": "~4.17.22",
        "@types/typeahead": "~0.11.27",
        "@types/handlebars": "~4.0.31",
        "@ng2/types": "~1.0.2",
        "rimraf": "~2.5.4",
        "typescript": "2.0.3",
        "webpack": "2.1.0-beta.25",
        "npm-publish-package": "~1.2.0"
    }
}